<script type="text/javascript">
$(document).ready(function() {
    latestNewsJSONAjax('http://news.abbvie.com/rss.cfm?section_id=16', true, function(){
        // alert(completeNewsResults);
        // alert("completeNewsResults ---- > " + completeNewsResults.Status);
        if( completeNewsResults.Status == 'Success') {

            selectionCount = newsCount(completeNewsResults.items,300);
            //selectionCount = "0";
            if (selectionCount.length > 3){
            	selectionCount = selectionCount.substring(0, 3);
            }else if (selectionCount.length == 1){
            	singleBigNews = true;
            }
        }else if( completeNewsResults.Status == 'Failure') {

			 selectionCount = "";
        }else {

			selectionCount = "";
            completeNewsResults.Status = 'Failure';
            completeNewsResults.StatusDescription = ' Server did not respond for the news request';

        }
		displayLogic();

    });
});
</script>
